&share 
 wrf_core = 'ARW',
 max_dom = 3,
 start_date = 'START_DATE','START_DATE','START_DATE',
 end_date   = 'END_DATE','END_DATE','END_DATE',
 interval_seconds = 21600,
 io_form_geogrid = 2,
/

&geogrid 
 parent_id         =    1,   1,   2, ! SA (Scandinavia Artic) domain
 parent_grid_ratio =    1,   3,   3,
 i_parent_start    =    1,  87,  39,
 j_parent_start    =    1, 133,  39, ! north-south
 e_we              =  250, 271, 559,
 e_sn              =  220, 211, 397,
 geog_data_res     =  'corine_100m+esa_cci+default','corine_100m+esa_cci+default','corine_100m+esa_cci+default',
 dx = 27000,
 dy = 27000,
 map_proj =  'lambert',
 ref_lat   =      54.0,
 ref_lon   =      15.0,
 truelat1  =      30.0,
 truelat2  =      60.0,
 stand_lon =      15.0,
 geog_data_path = '/mnt/aiolos/MET/WRF/geog/',
/

&ungrib 
  out_format = 'WPS',
  prefix = 'FIELD',
/

&metgrid 
  constants_name = 'TAVGSFC',
  fg_name = 'ERA','SST',
  io_form_metgrid = 2,
/
